{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "build",
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react",
    "lib": ["es2015", "dom", "es2017.object"]
  }
}
