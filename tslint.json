{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-any": true,
    "object-literal-sort-keys": false,
    "member-access": false,
    "no-console": [
      true,
      "log"
    ],
    "no-namespace": [
      true,
      "allow-declarations"
    ],
    "jsx-no-lambda": false
  }
}
