{
  "name": "veri-fit",
  "private": true,
  "description": "Website for veri-fit business",
  "version": "0.1.0",
  "author": "Fabio Berta <fnberta@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "typecheck": "tsc",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,sass,md,yml,yaml}'"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.10.16",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@fortawesome/fontawesome-free": "^5.7.2",
    "bulma": "^0.7.4",
    "classnames": "^2.2.6",
    "formik": "^1.5.1",
    "gatsby": "^2.1.27",
    "gatsby-image": "^2.0.31",
    "gatsby-plugin-emotion": "^4.0.4",
    "gatsby-plugin-netlify": "^2.0.11",
    "gatsby-plugin-netlify-cms": "^3.0.12",
    "gatsby-plugin-react-helmet": "^3.0.8",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-sharp": "^2.0.25",
    "gatsby-plugin-typescript": "^2.0.10",
    "gatsby-remark-images": "^3.0.6",
    "gatsby-remark-relative-images": "^0.2.2",
    "gatsby-source-filesystem": "^2.0.23",
    "gatsby-transformer-remark": "^2.3.1",
    "gatsby-transformer-sharp": "^2.1.15",
    "netlify-cms": "^2.6.0",
    "node-sass": "^4.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/react-helmet": "^5.0.8",
    "@typescript-eslint/eslint-plugin": "^1.4.2",
    "@typescript-eslint/parser": "^1.4.2",
    "eslint": "^5.15.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "typescript": "^3.3.3333"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,sass,md,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  }
}
