{
  "name": "veri-fit",
  "private": true,
  "description": "Website for veri-fit business",
  "version": "0.1.0",
  "author": "Fabio Berta <fnberta@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "typecheck": "tsc",
    "lint": "tslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,sass}'"
  },
  "dependencies": {
    "@brainhubeu/react-carousel": "^1.10.12",
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@fortawesome/fontawesome-free": "^5.7.1",
    "bulma": "^0.7.4",
    "classnames": "^2.2.6",
    "formik": "^1.5.0",
    "gatsby": "^2.0.118",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-emotion": "^4.0.3",
    "gatsby-plugin-netlify": "^2.0.10",
    "gatsby-plugin-netlify-cms": "^3.0.12",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sass": "^2.0.10",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-typescript": "^2.0.7",
    "gatsby-remark-images": "^3.0.3",
    "gatsby-remark-relative-images": "^0.2.1",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-remark": "^2.2.4",
    "gatsby-transformer-sharp": "^2.1.13",
    "netlify-cms": "^2.4.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/react-helmet": "^5.0.8",
    "gatsby-plugin-tslint": "^0.0.2",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "tslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,sass}": [
      "prettier --write",
      "git add"
    ]
  }
}
